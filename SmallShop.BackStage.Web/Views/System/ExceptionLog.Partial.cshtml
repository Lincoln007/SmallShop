@model List<ExceptionLogInfo>
@{
    if (Model == null || Model.Count == 0)
    {
        <tr><td colspan='100' class='nodata'>没有数据</td></tr>
    }
    else
    {
        foreach (var item in Model)
        {
            <tr>
                <td>@item.LoginName</td>
                <td>@item.Url</td>
                <td>@item.Ip</td>
                <td class="wordbreakall">@Html.Raw(item.Message)</td>
                <td>@item.CreateTime.ToDateTime()</td>                
                <td><a class="btn-sm btn-info" href="javascript:void(0);" onclick="showExceptionLogDetail(@item.Id)">显示详情</a></td>
            </tr>
        }
    }
}